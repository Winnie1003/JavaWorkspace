package hd;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a,b;
		Scanner sc = new Scanner(System.in);
		while(sc.hasNext()) {
			
			a = sc.nextInt();
			b = sc.nextInt();
			System.out.println((a+b));
		}
	}

}
